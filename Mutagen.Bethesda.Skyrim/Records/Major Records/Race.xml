    </Fields>
  </Object>
  <Object name="HeadPartReference" objType="Subrecord">
    <Fields>
      <Int32 name="Number" recordType="INDX" />
      <FormLink name="Head" refName="HeadPart" recordType="HEAD" />
    </Fields>
