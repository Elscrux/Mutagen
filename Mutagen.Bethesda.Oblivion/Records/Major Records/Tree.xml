<?xml version="1.0" encoding="us-ascii"?>
<Loqui xmlns="http://tempuri.org/LoquiSource.xsd">
  <Object name="Tree" baseClass="OblivionMajorRecord" objType="Record" recordType="TREE">
    <Fields>
      <RefDirect name="Model" refName="Model" />
      <String name="Icon" recordType="ICON" />
      <List name="SpeedTreeSeeds" recordType="SNAM">
        <UInt32 />
      </List>
      <RefDirect name="Data" refName="TreeData" />
      <RefDirect name="BillboardDimensions" refName="Dimensions" />
    </Fields>
  </Object>
  <Object name="TreeData" recordType="CNAM" objType="Subrecord">
    <Fields>
      <Float name="LeafCurvature" />
      <Float name="MinimumLeafAngle" />
      <Float name="MaximumLeafAngle" />
      <Float name="BranchDimmingValue" />
      <Float name="LeafDimmingValue" />
      <Int32 name="ShadowRadius" />
      <Float name="RockingSpeed" />
      <Float name="RustleSpeed" />
    </Fields>
  </Object>
  <Object name="Dimensions" recordType="BNAM" objType="Subrecord">
    <Fields>
      <Float name="Width" />
      <Float name="Height" />
    </Fields>
  </Object>
</Loqui>
