      <FormLink name="ConsumeSpell" refName="Spell" recordType="XNAM" />
      <FormLink name="ContactSpell" refName="Spell" recordType="YNAM" />
      <FormLink name="ImageSpace" refName="ImageSpace" recordType="INAM" />
