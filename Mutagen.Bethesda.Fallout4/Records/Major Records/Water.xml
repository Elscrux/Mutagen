          <Float name="PhysicalShallowNormalFalloff" />
          <Float name="PhysicalDeepNormalFalloff" />
          <Float name="PhysicalReflectivityAmount" />
          <Float name="PhysicalFresnelAmount" />
          <Float name="PhysicalSurfaceEffectFalloff" />
